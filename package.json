{
  "name": "swagger-typegen",
  "version": "1.1.2",
  "description": "Generate TypeScript types and API functions from Swagger/OpenAPI specification",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swagger-typegen": "./dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaofeiwuuu/swagger2ts-codegen.git"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "test": "node dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "api",
    "codegen",
    "swagger-typegen"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^12.0.0",
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "swagger-typegen": {
    "input": "./swagger-api.json",
    "output": "./src/api",
    "requestStyle": "chain",
    "requestClient": "requestClient",
    "requestImport": "@/utils/request",
    "pathPrefixFilter": [
      "/api/v1",
      "/api"
    ],
    "typeNameSuffixFilter": [
      "Request",
      "Response",
      "Req",
      "Res"
    ],
    "excludeFields": [],
    "tagMapping": {},
    "includeTags": [],
    "excludeTags": []
  }
}
